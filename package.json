{
  "name": "guidesnotincluded",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "pnpm 'fmt:check' && pnpm lint && pnpm typecheck && pnpm test",
    "dev": "concurrently \"content-collections watch\" \"next dev\"",
    "generate": "content-collections build",
    "build": "content-collections build && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "fmt:check": "prettier --check .",
    "fmt": "prettier --write .",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@content-collections/cli": "^0.1.7",
    "@content-collections/core": "^0.11.1",
    "@content-collections/next": "^0.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20.19.12",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "zod": "^4.1.5"
  }
}
